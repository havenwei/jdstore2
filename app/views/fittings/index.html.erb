  <div id="third-1">
    <% @fittings.each do |fitting| %>

      <div id="third-11", style="margin-bottom:30px;">
        <%= link_to fitting_path(fitting) do %>
          <% if fitting.image.present? %>
            <%= image_tag(fitting.image.thumb.url, class:"img-rounded") %>
          <% else %>
            <%= image_tag(url_to_image('NoPic'), class:"img-rounded") %>
          <% end %>
        <% end %>
        <br>
        <h5><%= fitting.title %> ￥ <%= fitting.price %></h5>
    </div>
    <% end %>
  </div>
